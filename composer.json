{
    "name": "wrklst/zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "http://github.com/wrklst/zipper",
    "license": "Apache2",
    "authors": [
        {
            "name": "Nils Plaschke",
            "email": "github@nilsplaschke.de",
            "homepage": "http://nilsplaschke.de",
            "role": "Developer"
        },
        {
            "name": "Tobias Vielmetter-Diekmann",
            "email": "tobias@wrklst.art",
            "homepage": "https://artworldsolutions.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Wrklst\\Zipper\\": "src/Wrklst/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wrklst\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wrklst\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Wrklst\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
